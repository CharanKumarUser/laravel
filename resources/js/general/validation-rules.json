  },
  {
    "key": "asset-id",
    "regex": "^[A-Z]{3}\\d{6}$",
    "error": "Must be 3 letters followed by 6 digits (e.g., AST123456)",
    "allowedChars": "[A-Z\\d]"
  },
  {
    "key": "kyc-id",
    "regex": "^[A-Z]{3}\\d{8}$",
    "error": "Must be 3 letters followed by 8 digits (e.g., KYC12345678)",
    "allowedChars": "[A-Z\\d]"
  },
  {
    "key": "tin",
    "regex": "^\\d{9}$",
    "error": "Must be a 9-digit TIN",
    "allowedChars": "\\d"
  },
  {
    "key": "cin",
    "regex": "^[LU][0-9]{5}[A-Z]{2}[0-9]{4}[A-Z]{3}[0-9]{6}$",
    "error": "Must be a valid 21-character CIN (e.g., L12345MH2000PLC123456)",
    "allowedChars": "[A-Z0-9]"
  },
  {
    "key": "msme_udyam",
    "regex": "^UDYAM-[A-Z]{2}-[0-9]{2}-[0-9]{7}$",
    "error": "Invalid UDYAM Number (e.g., UDYAM-MH-12-0000001)",
    "cleave": {
      "blocks": [
        5,
        2,
        2,
        7
      ],
      "delimiter": "-"
    },
    "allowedChars": "[A-Z0-9-]"
  },
  {
    "key": "roc",
    "regex": "^\\d{6}$",
    "error": "Company registration number must be a 6-digit number",
    "cleave": {
      "blocks": [
        6
      ],
      "delimiter": ""
    },
    "allowedChars": "[\\d]"
  },
  {
    "key": "din",
    "regex": "^\\d{8}$",
    "error": "Must be an 8-digit DIN",
    "allowedChars": "\\d"
  },
  {
    "key": "tan",
    "regex": "^[A-Z]{4}[0-9]{5}[A-Z]{1}$",
    "error": "Must be 4 letters, 5 digits, 1 letter (e.g., ABCD12345E)",
    "allowedChars": "[A-Z0-9]"
  },
  {
    "key": "permission",
    "regex": "^(create|view|edit|delete|import|export):[A-Za-z]+(?:::[A-Za-z\\s]+)*$",
