    },
    "allowedChars": "[\\d.]"
  },
  {
    "key": "imei",
    "regex": "^\\d{15}$",
    "error": "Must be a 15-digit IMEI",
    "allowedChars": "\\d"
  },
  {
    "key": "mac-address",
    "regex": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$",
