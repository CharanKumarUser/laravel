    "cleave": {
      "delimiter": ":",
      "blocks": [
        2,
        2,
        2,
        2,
        2,
        2
      ],
      "uppercase": true
    },
    "allowedChars": "[0-9A-Fa-f:-]"
  },
  {
    "key": "serial-number",
    "regex": "^[A-Z0-9]{6,20}$",
